menu tools /usr/share/icons/HighContrast/32x32/emblems/emblem-favorite.png {
prog XTerm xterm x-terminal-emulator
#prog FTE fte fte
#prog Netscape netscape netscape
prog    "gvim" /usr/X11R6/include/X11/pixmaps/vim.xpm /usr/bin/gvim -f
prog    "chromium" /usr/share/pixmaps/chromium-browser.png chromium-browser
prog "pd" /usr/share/pixmaps/puredata.xpm pd
